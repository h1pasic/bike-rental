  "jsxSingleQuote": true,
  "printWidth": 200,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "trailingComma": "all",
  "singleQuote": true,
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}